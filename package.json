{
   "author": "SunYufei",
   "description": "WebDAV supports for Google Drive using Cloudflare Workers",
   "devDependencies": {
      "@cloudflare/workers-types": "3",
      "@types/jest": "27",
      "jest": "27",
      "prettier": "2",
      "ts-jest": "27",
      "ts-loader": "8",
      "typescript": "4",
      "webpack": "5",
      "webpack-cli": "4"
   },
   "homepage": "https://github.com/SunYufei/workers-dav#readme",
   "license": "MIT",
   "main": "dist/worker.js",
   "name": "workers-dav",
   "prettier": {
      "printWidth": 80,
      "quoteProps": "preserve",
      "semi": false,
      "singleQuote": true,
      "tabWidth": 3,
      "trailingComma": "es5",
      "useTabs": false
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/SunYufei/workers-dav.git"
   },
   "scripts": {
      "build": "wrangler build",
      "format": "prettier --write  *.{json,js} src/**/*.{json,js,ts} test/**/*.{js,ts}",
      "publish": "wrangler publish",
      "preview": "wrangler preview --watch",
      "test": "jest"
   },
   "version": "0.1.0"
}

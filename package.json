{
   "version": "0.2.0",
   "author": "SunYufei",
   "description": "WebDAV supports for Google Drive using Cloudflare Workers",
   "homepage": "https://github.com/SunYufei/workers-dav#readme",
   "name": "workers-dav",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/SunYufei/workers-dav.git"
   },
   "scripts": {
      "dev": "wrangler dev --local",
      "format": "prettier --write  *.{json,js} src/**/*.{json,js,ts} test/**/*.{js,ts}",
      "prod": "wrangler publish",
      "test": "jest"
   },
   "devDependencies": {
      "@cloudflare/workers-types": "3",
      "@miniflare/core": "2",
      "prettier": "2",
      "wrangler": "3"
   },
   "prettier": {
      "printWidth": 80,
      "quoteProps": "preserve",
      "semi": false,
      "singleQuote": true,
      "tabWidth": 3,
      "trailingComma": "es5",
      "useTabs": false
   },
   "license": "MIT"
}

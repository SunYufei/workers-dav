{
   "compilerOptions": {
      "module": "CommonJS",
      "target": "ES6",
      "lib": ["ES6"],
      "alwaysStrict": true,
      "strict": true,
      "preserveConstEnums": true,
      "moduleResolution": "Node",
      "noImplicitAny": true,
      "removeComments": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "types": ["@cloudflare/workers-types", "@types/jest"]
   },
   "include": ["src"],
   "exclude": ["node_modules", "test", "dist", "worker"]
}

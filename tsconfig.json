{
   "compilerOptions": {
      "module": "ES2022",
      "target": "ES2022",
      "lib": ["ES2022"],
      "strict": true,
      "preserveConstEnums": true,
      "moduleResolution": "Node",
      "noImplicitAny": true,
      "removeComments": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "typeRoots": ["node_modules"],
      "types": ["@cloudflare/workers-types"]
   },
   "include": ["src/**/*.ts"],
   "exclude": ["node_modules", "test", "dist", "worker"]
}
